{
    "name": "@leadhome/eslint-config",
    "version": "1.0.4",
    "description": "A shareable ESLint configuration",
    "main": ".eslintrc",
    "scripts": {
        "commit": "npx git-cz"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leadhomesa/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/leadhomesa/eslint-config/issues"
    },
    "author": "Leadhome <info@leadhome.co.za> (https://leadhome.co.za)",
    "license": "GPL-2.0+",
    "homepage": "https://github.com/leadhomesa/eslint-config#readme",
    "keywords": [
        "eslint",
        "eslint-config",
        "leadhome"
    ],
    "dependencies": {
        "babel-eslint": "^10.0.3",
        "cz-conventional-changelog": "^3.0.2",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-config-standard-react": "^9.2.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-standard": "^4.0.1",
        "eslint-plugin-unicorn": "^14.0.1",
        "eslint-plugin-react-hooks": "^2.3.0",
        "prettier": "^1.19.1"
    },
    "eslintConfig": {
        "extends": "./.eslintrc"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
